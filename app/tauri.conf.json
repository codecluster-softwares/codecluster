{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "app",
  "version": "0.0.0",
  "identifier": "cloud.codecluster.app",
  "build": {
    "beforeDevCommand": "pnpm vite dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm vue-tsc --noEmit && pnpm vite build",
    "frontendDist": "../out"
  },
  "app": {
    "windows": [{ "title": "Code Cluster", "width": 800, "height": 600 }],
    "security": { "csp": null }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
